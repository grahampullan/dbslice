{
  "metaData": "./data/metaData.csv",
  "metaDataCsv": true,
  "generateTaskIds": true,
  "taskIdRoot": "case_",
  "taskIdFormat": "d",
  "setLabelsToTaskIds": true,
  "title": "3D box of data demo",
  "config": {
    "plotTasksButton": true,
    "saveTasksButton": false
  },
  "plotRows": [
    {
      "title": "3D box database",
      "addPlotButton": true,
      "plots": [
        {
          "plotType": "cfD3BarChart",
          "data": {
            "property": "Simulation type"
          },
          "layout": {
            "title": "Simulation",
            "colWidth": 3,
            "height": 300,
            "highlightTasks": true,
            "removePlotButton": false,
			"configurePlotButton": true
          }
        },
        {
          "plotType": "cfD3BarChart",
          "data": {
            "property": "Model type"
          },
          "layout": {
            "title": "Model",
            "colWidth": 3,
            "height": 300,
            "highlightTasks": true,
            "removePlotButton": false,
			"configurePlotButton": true
          }
        },
        {
          "plotType": "cfD3Scatter",
          "data": {
            "xProperty": "Average f",
            "yProperty": "Std dev f",
            "cProperty": "Model type"
          },
          "layout": {
            "title": "Std dev f",
            "colWidth": 3,
            "height": 300,
            "highlightTasks": true,
            "removePlotButton": false,
			"configurePlotButton": true
          }
        },
        {
          "plotType": "cfD3Histogram",
          "data": {
            "property": "Average f"
          },
          "layout": {
            "title": "Average",
            "colWidth": 3,
            "height": 300,
            "highlightTasks": true,
            "removePlotButton": false,
			"configurePlotButton": true
          }
        }
      ]
    },
    {
      "title": "Line plots",
      "addPlotButton": false,
      "plots": [
        {
          "plotType": "d3LineSeries",
          "layout": {
            "title": "Start",
            "colWidth": 4,
            "height": 250,
            "highlightTasks": true,
            "cSet": "Model type",
            "removePlotButton": false
          },
          "fetchData": {
            "urlTemplate": "./data/${taskId}/f_line_xstart.json",
            "tasksByFilter": true,
            "maxTasks": 50,
            "dataFilterType": "lineSeriesFromLines",
            "dataFilterConfig": {
              "cProperty": "Model type"
            }
          }
        },
        {
          "plotType": "d3LineSeries",
          "layout": {
            "title": "Mid",
            "colWidth": 4,
            "height": 250,
            "highlightTasks": true,
            "cSet": "Model type",
            "removePlotButton": false
          },
          "fetchData": {
            "urlTemplate": "./data/${taskId}/f_line_xmid.json",
            "tasksByFilter": true,
            "maxTasks": 50,
            "dataFilterType": "lineSeriesFromLines",
            "dataFilterConfig": {
              "cProperty": "Model type"
            }
          }
        },
        {
          "plotType": "d3LineSeries",
          "layout": {
            "title": "End",
            "colWidth": 4,
            "height": 250,
            "highlightTasks": true,
            "cSet": "Model type",
            "removePlotButton": false
          },
          "fetchData": {
            "urlTemplate": "./data/${taskId}/f_line_xend.json",
            "tasksByFilter": true,
            "maxTasks": 50,
            "dataFilterType": "lineSeriesFromLines",
            "dataFilterConfig": {
              "cProperty": "Model type"
            }
          }
        }
      ]
    },
    {
      "title": "f at x=0",
      "plots": [],
      "ctrl": {
        "plotType": "d3ContourStruct2d",
        "layout": {
          "colWidth": 3,
          "height": 300,
          "vScale": [
            -0.1,
            1.2
          ],
          "highlightTasks": true
        },
        "urlTemplate": "./data/${taskId}/f_area2d_xstart.json",
        "tasksByFilter": true,
        "maxTasks": 20
      }
    },
    {
      "title": "f at x=0, x=mid, x=end",
      "plots": [],
      "ctrl": {
        "plotType": "threeSurf3d",
        "layout": {
          "colWidth": 4,
          "height": 400,
          "vScale": [
            -0.1,
            1.2
          ],
          "highlightTasks": true,
          "cameraSync": true
        },
        "urlTemplate": "./data/${taskId}/f_area3d.json",
        "tasksByFilter": true,
        "maxTasks": 5,
        "dataFilterType": "threeMeshFromStruct"
      }
    }
  ]
}